<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="../stylesheet/main.css" />
    <script src="../scripts/dialogControl.js" defer></script>
  </head>
  <body>
    <header></header>
    <main>
      <span class="material-symbols-outlined"> add </span>
      <span class="material-symbols-outlined open-button">
        create_new_folder
      </span>
      <section class="form-section">
        <form action="/" method="POST" enctype="multipart/form-data">
          <div class="form-group">
            <label for="file_upload">Choose file: </label>
            <input type="file" name="file_upload" id="file_upload" multiple />
          </div>
          <div class="button-container">
            <button type="submit">Submit</button>
          </div>
        </form>
      </section>
      <%- include('./partials/folder') %> <%- include('./partials/modal',
      {parent: parent}) %>
    </main>
  </body>
  <footer></footer>
</html>
