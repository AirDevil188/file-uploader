<% if (currentFolder) { %>
<section class="folder-section">
  <span><a href=""><%= currentUrl %></a></span>
  <% folders.map(folder => { %>
  <div class="folder" id="<%= folder.id %>">
    <div class="icon">
      <span class="material-symbols-outlined"> folder </span>
    </div>
    <div class="folder-name-container">
      <span class="folder-name">
        <a href="<%= folder.path%>"><%= folder.name %></a></span
      >
    </div>
  </div>
  <% }) %>
  <div class="folder" id="folder.id"></div>
</section>

<% } else { %> <%- include('InvalidPage') %> <% } %>
